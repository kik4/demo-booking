{
  "name": "demo-booking",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "format": "biome check --write --unsafe .",
    "prepare": "husky",
    "type:check": "tsc --noEmit",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:diff": "supabase db diff",
    "supabase:type": "supabase gen types typescript --local > types/database.types.ts",
    "supabase:sample": "tsx ./tasks/setSampleSupabase.ts "
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "clsx": "^2.1.1",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@next/env": "^15.3.3",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^20.19.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "supabase": "^2.24.3",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*": [
      "biome check --write --unsafe --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
